{
  "name": "smpark_space",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/smpark_space",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "smpark_space:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "smpark_space:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "smpark_space:build:production",
          "dev": false
        }
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "root": "apps/smpark_space",
        "outputPath": "dist/apps/smpark_space"
      }
    }
  }
}